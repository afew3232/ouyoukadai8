<div class="row" style="text-align:center">
	<div>
		<%= form_tag(search_search_path, method: :get) do %>
		<%= text_field_tag :word %>
		<%= select_tag(:target, options_for_select([["User", "User"], ['Book', 'Book']])) %>
		<%= select_tag(:style, options_for_select([["完全一致", "完全一致"], ['前方一致', '前方一致'],['後方一致','後方一致'],["部分一致","部分一致"]])) %>
		<%= submit_tag "検索" %>
		<% end %>
	</div>
</div>